
function onStart() {

    someMethod($myVariable);
    anotherMethod ();
}

function loop() {

    if_($initialized != 1)
        shoot();
        turLeft();
        shoot();
        turnLeft();
        shoot();
        turnLeft();
        shoot();
    endif;

    $initialized = 1;

    scanForward();

    if_($scanResult == BARRIER)
        turnRandom();
        loop();
    endif;

    if_($scanResult == MINE)
        shoot();
        loop();
    endif;

    if_($scanResult == EMPTY)
        move();
    endif;
}